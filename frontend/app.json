{
	"expo": {
		"name": "QVC App",
		"slug": "qvc-app",
		"version": "2.0.0",
		"owner": "khanhismes-organization",
		"orientation": "default",
		"icon": "./assets/icon.png",
		"scheme": "qvcapp",
		"userInterfaceStyle": "automatic",
		"runtimeVersion": {
			"policy": "sdkVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/8ee4dd7a-7fb3-4638-a6e9-576f8df943a4"
		},
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#2563EB"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.quocviet.superapp",
			"buildNumber": "1",
			"infoPlist": {
				"NSCameraUsageDescription": "Camera được sử dụng để xác thực khuôn mặt khi chấm công",
				"NSLocationWhenInUseUsageDescription": "Vị trí được sử dụng để xác minh bạn đang ở văn phòng khi chấm công"
			}
		},
		"android": {
			"package": "com.quocviet.superapp",
			"versionCode": 1,
			"permissions": [
				"CAMERA",
				"ACCESS_FINE_LOCATION",
				"ACCESS_COARSE_LOCATION"
			],
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#2563EB"
			}
		},
		"plugins": [
			"expo-router",
			[
				"expo-camera",
				{
					"cameraPermission": "Camera được sử dụng để xác thực khuôn mặt khi chấm công"
				}
			],
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Vị trí được sử dụng để xác minh bạn đang ở văn phòng khi chấm công"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "8ee4dd7a-7fb3-4638-a6e9-576f8df943a4"
			}
		}
	}
}